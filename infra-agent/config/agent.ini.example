[Server]
# Backend server URL (required)
url = https://api.infrawatch.website
# VM ID from Infrawatch (required)
vm_id = YOUR_VM_ID_HERE
# Agent token for authentication (required)
agent_token = YOUR_AGENT_TOKEN_HERE

[Intervals]
# Monitoring data collection interval in seconds
monitoring = 60
# Heartbeat sending interval in seconds
heartbeat = 30

[Logging]
# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
level = INFO
# Log file path
file = ./logs/agent.log