# API URL for development server
# This will override the API URL in .env for development environment
# Use api.infrawatch.website for remote server
REACT_APP_API_URL=https://api.infrawatch.website/api/v1

# Set to 'false' to use the real backend API
REACT_APP_USE_MOCK_DATA=false

# Set to 'false' to use real terminal connections
REACT_APP_TERMINAL_TEST_MODE=false

# Set to 'true' to use WebSocket proxy through the current host
REACT_APP_USE_WS_PROXY=true

# Development port - match with CORS configuration on server
PORT=8000
